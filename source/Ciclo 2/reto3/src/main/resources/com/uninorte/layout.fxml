<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="appWindow" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="com.uninorte.controllers.retoController">
    <children>
        <MenuBar fx:id="appMenu" VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Archivo">
                    <items>
                        <MenuItem fx:id="clearMenuItem" mnemonicParsing="false"
                            onAction="#clearFields" text="Limpiar Todo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" onAction="#closeApp"
                            text="Cerrar" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="ALWAYS" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Nombre" GridPane.halignment="RIGHT" GridPane.rowIndex="1"
                    GridPane.valignment="CENTER" />
                <Label text="Municipio" GridPane.halignment="RIGHT" GridPane.rowIndex="2"
                    GridPane.valignment="CENTER" />
                <Label text="Tipo de Agua" GridPane.halignment="RIGHT" GridPane.rowIndex="3"
                    GridPane.valignment="CENTER" />
                <Label text="ID" GridPane.columnIndex="2" GridPane.halignment="RIGHT"
                    GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                <Label text="Tipo de Cuerpo de Agua" GridPane.columnIndex="2"
                    GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
                <Label text="Valor IRCA" GridPane.columnIndex="2" GridPane.halignment="RIGHT"
                    GridPane.rowIndex="3" GridPane.valignment="CENTER" />
                <TextField fx:id="nameField" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="stateField" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="idField" GridPane.columnIndex="3" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="geographyTypeField" GridPane.columnIndex="3" GridPane.rowIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="ircaField" GridPane.columnIndex="3" GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextField>
                <Button fx:id="addButton" alignment="CENTER" contentDisplay="CENTER"
                    mnemonicParsing="false" onAction="#addCuerpoAgua" text="Agregar Cuerpo"
                    GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.rowIndex="4"
                    GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </GridPane.margin>
                </Button>
                <Label text="Datos de Cuerpo de Agua" GridPane.columnSpan="5">
                    <GridPane.margin>
                        <Insets />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <ComboBox fx:id="waterTypeSelector" prefWidth="150.0" GridPane.columnIndex="1"
                    GridPane.rowIndex="3">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </ComboBox>
            </children>
            <VBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
        </GridPane>
        <TableView fx:id="dataTable" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="idColumn" editable="false" prefWidth="48.0" sortable="false"
                    text="ID" />
                <TableColumn fx:id="nameColumn" editable="false" prefWidth="206.0" sortable="false"
                    text="Nombre" />
                <TableColumn fx:id="stateColumn" editable="false" minWidth="0.0" prefWidth="134.0"
                    sortable="false" text="Municipio" />
                <TableColumn fx:id="geographyTypeColumn" editable="false" prefWidth="152.0"
                    sortable="false" text="Tipo de Cuerpo " />
                <TableColumn fx:id="waterTypeColumn" editable="false" prefWidth="132.0"
                    sortable="false" text="Tipo de Agua" />
                <TableColumn fx:id="ircaColumn" editable="false" prefWidth="107.0" sortable="false"
                    text="IRCA" />
            </columns>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </TableView>
        <GridPane prefHeight="100.0" prefWidth="760.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="44.0" minHeight="10.0" prefHeight="36.0"
                    vgrow="SOMETIMES" />
                <RowConstraints maxHeight="72.0" minHeight="10.0" prefHeight="64.0"
                    vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextArea fx:id="resultsArea" editable="false" prefHeight="200.0" prefWidth="200.0"
                    promptText="Resultados" GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </TextArea>
                <Button fx:id="analyzeButton" mnemonicParsing="false" onAction="#analizeTableData"
                    text="Analizar" textAlignment="CENTER" GridPane.columnIndex="1"
                    GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
                    <GridPane.margin>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                    </GridPane.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </GridPane>
    </children>
</VBox>